{
  "model_name": "strategy_generator",
  "base_model": "meta-llama/Llama-3.1-8B",
  "version": "1.0.0",
  "optimization_target": "snapdragon_elite_npu",
  "deployment_config": {
    "task": "fire_suppression_strategy",
    "input_format": "situation_analysis + available_resources",
    "output_format": "action_sequence_json",
    "max_input_tokens": 1536,
    "max_output_tokens": 768,
    "temperature": 0.5,
    "specialization": "tactical_planning"
  },
  "performance_targets": {
    "max_latency_ms": 300,
    "max_memory_mb": 2048,
    "min_throughput_tokens_per_sec": 50
  },
  "cirrascale_training": {
    "epochs": 3,
    "batch_size": 2,
    "learning_rate": 5e-06,
    "lora_r": 16,
    "lora_alpha": 32
  }
}